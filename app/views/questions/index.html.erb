<h1>
  <%= t('questions_page.h1') %>
</h1>

<ul>
  <% @questions.each do |question| %>
    <li>
      <%= link_to question.body, question_path(question) %>
      <%= link_to t('questions_page.delete_question'), question_path(question), method: 'delete' %>
    </li>
  <% end %>
</ul>

<%= link_to t('questions_page.add_question'), new_test_question_path(@test) %>
